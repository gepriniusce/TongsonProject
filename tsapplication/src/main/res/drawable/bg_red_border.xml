<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <!-- 填充的颜色 -->
            <solid android:color="#f6e7e5"/>

            <!--描边-->
            <stroke
                android:width="1px"
                android:color="#eae3d4"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <!-- 填充的颜色 -->
            <solid android:color="#f2f2f1"/>

            <!--描边-->
            <stroke
                android:width="1px"
                android:color="#252025"/>
        </shape>
    </item>
</selector>